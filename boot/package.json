  "scripts": {
    "compile": "gulp compile",
    "start:bin": "node --trace-deprecation bin/micro-up-boot start --config ./project.config.js -s src",
    "pre-publish": "node bin/micro-up-boot prepublish -p ./package.json",
    "dist:bin": "node --trace-deprecation bin/micro-up-boot dist --config ./project.config.js -s src",
    "dist-dev:bin": "node --trace-deprecation bin/micro-up-boot dist --config ./project.config.js -s src -d true",
    "compile-external": "node bin/micro-up-boot compile --external",
    "lint": "npm run lint:es && npm run lint:style",
    "lint:es": "eslint src/**/*.{js,jsx} react ./project.config.js",
    "lint:style": "stylelint src/**/*.{less,css}",
    "lint-staged": "lint-staged",
    "lint-staged:es": "eslint src/**/*.{js,jsx} react/**/*.{js,jsx} ./project.config.js --ext '.js,.jsx'",
    "test-http-server": "http-server ./dist",
    "prestart:bin": "npm run compile",
    "predist:bin": "npm run compile",
    "predist-dev:bin": "npm run compile",
    "prepre-publish": "npm run compile"
  },
